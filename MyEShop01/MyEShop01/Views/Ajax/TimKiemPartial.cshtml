@model IEnumerable<MyEShop01.Models.KetQuaTimKiemVM>

<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.MaHh)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.TenHh)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DonGia)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.NgaySX)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Loai)
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.MaHh)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.TenHh)
				</td>
				<td>@item.DonGia.ToString("#,##0.00") đ</td>
				<td>@item.NgaySX.ToString("dd/MM/yyyy")</td>
				<td>
					@Html.DisplayFor(modelItem => item.Loai)
				</td>
			</tr>
		}
	</tbody>
</table>
